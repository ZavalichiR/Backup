package tema5;

public class MyThread extends Thread{
	private int stop;
	private int suma;
	static Object o=new Object();
	MyThread(int stop){
		this.stop=stop;
	}
	public void run(){
		for(int i=0;i<=stop;++i)
		{	
			this.suma+=i;
		}
		System.out.println("RE: "+suma);
	}
	public int GetSuma(){
		return this.suma;
	}
}
