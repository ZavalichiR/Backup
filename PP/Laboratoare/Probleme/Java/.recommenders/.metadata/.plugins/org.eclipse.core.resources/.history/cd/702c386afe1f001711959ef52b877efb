package tema4;

import tema4.MyThread;

public class Main {

	public static void main(String[] args) {
		MyThread t1=new MyThread(1,5); 
		MyThread t2=new MyThread(6,10);
		MyThread t3=new MyThread(11,15);
		MyThread t4=new MyThread(16,20);
		MyThread.suma=0;
		t1.start();
		t2.start();
		t3.start();
		t4.start();
	}
}
