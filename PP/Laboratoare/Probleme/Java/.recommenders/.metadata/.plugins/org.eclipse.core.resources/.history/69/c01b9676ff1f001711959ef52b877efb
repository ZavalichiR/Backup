package tema4;

import tema4.MyThread;

public class Main {

	public static void main(String[] args) {
		MyThread t1=new MyThread(1,3); 
		MyThread t2=new MyThread(4,6);
		MyThread t3=new MyThread(7,10);
		MyThread t4=new MyThread(10,20);
		MyThread.suma=0;
		t1.start();
		t2.start();
		t3.start();
		t4.start();
	}
}
